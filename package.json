{
  "name": "medical-screening-system",
  "version": "1.0.0",
  "description": "AI-powered medical screening platform for anemia detection",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest --verbose --forceExit",
    "test:auth": "jest tests/auth.test.js --verbose --forceExit",
    "test:api": "jest tests/api.test.js --verbose --forceExit",
    "test:model": "jest tests/model.test.js --verbose --forceExit",
    "test:integration": "jest tests/integration.test.js --verbose --forceExit",
    "test:watch": "jest --watch --verbose",
    "test:coverage": "jest --coverage --verbose --forceExit",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand --verbose",
    "test:setup": "node tests/setup.js",
    "setup:db": "node scripts/setup-db.js",
    "create:admin": "node scripts/create-admin.js",
    "seed:data": "node scripts/seed-data.js",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "debug": "node debug-server.js",
    "docs": "jsdoc -c jsdoc.conf.json",
    "backup:db": "node scripts/backup-db.js",
    "deploy:vercel": "vercel --prod",
    "health": "node -e \"require('./server'); setTimeout(() => require('http').get('http://localhost:3000/health', (res) => { console.log('Health check:', res.statusCode); process.exit(0); }), 2000)\""
  },
  "keywords": [
    "medical",
    "ai",
    "screening",
    "anemia",
    "healthcare",
    "malaria",
    "machine-learning",
    "web-app"
  ],
  "author": "Medical Screening Team",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "connect-mongo": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "express-session": "^1.17.3",
    "helmet": "^7.0.0",
    "mongoose": "^8.0.3",
    "multer": "^1.4.5-lts.1",
    "onnxruntime-node": "^1.22.0-rev",
    "onnxruntime-web": "^1.22.0",
    "sharp": "^0.33.5"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.5",
    "cross-env": "^7.0.3",
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "jsdoc": "^4.0.2",
    "mongodb-memory-server": "^8.15.1",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "supertest": "^6.3.4"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/medical-screening-system.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/medical-screening-system/issues"
  },
  "homepage": "https://github.com/yourusername/medical-screening-system#readme",
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup.js"
    ],
    "testTimeout": 30000,
    "verbose": true,
    "collectCoverage": false,
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ],
    "testMatch": [
      "**/tests/**/*.test.js",
      "**/?(*.)+(spec|test).js"
    ],
    "collectCoverageFrom": [
      "server.js",
      "models/**/*.js",
      "routes/**/*.js",
      "middleware/**/*.js",
      "!node_modules/**",
      "!coverage/**",
      "!tests/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50,
        "statements": 50
      }
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/coverage/",
      "/uploads/",
      "/patient_data/"
    ]
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "es2021": true,
      "jest": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "rules": {
      "no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "^_"
        }
      ],
      "no-console": "off",
      "prefer-const": "error",
      "no-var": "error"
    }
  },
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 80,
    "tabWidth": 2
  }
}
